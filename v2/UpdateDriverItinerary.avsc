{
  "type" : "record",
  "name" : "UpdateDriverItinerary",
  "namespace" : "com.stuart.commands.v2",
  "doc" : "* Represents a command to update the itinerary for a given driver",
  "fields" : [ {
    "name" : "driver_id",
    "type" : "long"
  }, {
    "name" : "waypoints",
    "type" : {
