    "name" : "driver_id",
    "type" : "long"
  }, {
    "name" : "transport_type",
    "type" : {
      "type" : "enum",
      "name" : "TransportType",
      "doc" : "* The set of available Transport Types",
      "symbols" : [ "WALK", "BIKE", "MOTORBIKE", "CAR", "CARGOBIKE", "VAN", "CARGOBIKEXL", "MOTORBIKEXL", "STEERWAGON", "TRUCK", "UNKNOWN", "TRAILERBIKE", "E_BIKE", "E_MOTORBIKE", "E_CAR", "E_VAN", "E_MOTORBIKEXL", "E_TRUCK" ],
      "default" : "UNKNOWN"
    }
  }, {
    "name" : "fleets",
    "type" : {
      "type" : "array",
      "items" : "long"
    }
  } ]
}
